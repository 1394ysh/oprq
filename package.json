{
  "name": "openapi-rq",
  "version": "0.1.3",
  "description": "OpenAPI to React Query code generator with shadcn-style DX",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "openapi-rq": "./bin/orq.js",
    "orq": "./bin/orq.js"
  },
  "files": [
    "dist",
    "bin",
    "schema.json",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openapi",
    "react-query",
    "tanstack-query",
    "code-generator",
    "cli",
    "typescript",
    "api-client",
    "axios",
    "shadcn"
  ],
  "author": "rocky <1394ysh@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1394ysh/openapi-rq.git"
  },
  "homepage": "https://github.com/1394ysh/openapi-rq#readme",
  "bugs": {
    "url": "https://github.com/1394ysh/openapi-rq/issues"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.3.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.14.0",
    "tsx": "^4.15.0",
    "typescript": "^5.5.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=4.0.0",
    "react-query": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "react-query": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
